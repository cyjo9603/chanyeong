{
  "name": "chanyeong_front",
  "version": "1.0.0",
  "description": "chanyeong web frontend server",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --exec node server.js",
    "wbuild": "next build",
    "prewstart": "npm run wbuild",
    "wstart": "cross-env NODE_ENV=production PORT=80 node server.js",
    "build": "next build",
    "prestart": "npm run build",
    "start": "NODE_ENV=production PORT=3333 pm2 start server.js",
    "pretypes": "graphql-codegen",
    "types": "apollo codegen:generate src/types/api.d.ts --queries=src/queries/*.queries.ts --addTypename --localSchemaFile=schema.json --target typescript --outputFlat",
    "storybook": "start-storybook -p 6006 -c .storybook"
  },
  "keywords": [],
  "author": "chanyeong",
  "license": "MIT",
