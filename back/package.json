    "types": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts",
    "start": "NODE_ENV=production PORT=80 pm2 start ./src.index.ts"
  },
  "keywords": [],
  "author": "chanyeong",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^2.14.1",
    "bcrypt": "^4.0.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-tools": "^6.0.5",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.8",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.11",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
