    "@typescript-eslint/parser": "2.31.0",
    "babel-runtime": "^6.26.0",
    "eslint": "7.0.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "gql-merge": "0.0.6",
    "graphql-to-typescript": "^0.1.2",
    "nodemon": "^2.0.4",
