{
  "name": "chanyeong_back",
  "version": "1.0.0",
  "description": "chanyeong web backend server",
  "main": "index.ts",
  "scripts": {
    "predev": "npm run types",
    "dev": "nodemon --exec ./node_modules/.bin/ts-node ./src/index.ts",
